{
  "name": "yaasoft/yii2-yaa-cms",
  "description": "Yaa CMS Based on Yii 2 Advanced Project Template",
  "keywords": [
    "yaa",
    "cms",
    "yii2"
  ],
  "homepage": "",
  "type": "project",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "yaasoft",
      "email": "yaasoft@gmail.com"
    }
  ],
  "support": {
    "source": "https://github.com/yaasoft/yii2-yaa-cms"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-bootstrap": "~2.0.6",
    "yiisoft/yii2-swiftmailer": "~2.0.6",
    "yeesoft/yii2-yee-core": "~0.1.0",
    "yeesoft/yii2-yee-auth": "~0.1.0",
    "yeesoft/yii2-yee-user": "~0.1.0",
    "yeesoft/yii2-yee-settings": "~0.1.0",
    "yeesoft/yii2-yee-menu": "~0.1.0",
    "yeesoft/yii2-yee-translation": "~0.1.0",
    "yeesoft/yii2-yee-post": "~0.1.0",
    "yeesoft/yii2-yee-media": "~0.1.0",
    "yeesoft/yii2-yee-page": "~0.1.0",
    "yeesoft/yii2-yee-seo": "~0.1.0",
    "yeesoft/yii2-comments": "~0.1.0",
    "yeesoft/yii2-yee-comment": "~0.1.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0.5",
    "yiisoft/yii2-debug": "~2.0.9",
    "yiisoft/yii2-gii": "~2.0.5",
    "yiisoft/yii2-faker": "~2.0.3"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
